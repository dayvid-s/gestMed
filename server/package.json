{
  "name": "gestmed",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "migration:create": "cd ./src/migrations && typeorm-ts-node-commonjs migration:create",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:generate ./src/migrations/default",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:run ",
    "migration:revert": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:revert --all",
    "build": "tsc",
    "dev": "npm run build && pm2 start dist/index.js --watch",
    "start": "npm run build && pm2 start dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.1",
    "nodemon": "^3.1.0",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.3",
    "pm2": "^5.3.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.7"
  }
}